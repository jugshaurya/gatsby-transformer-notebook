{
  "name": "gatsby-transformer-notebook",
  "version": "1.0.0",
  "description": "A gatsby transformer to convert notebook into html.",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\"",
    "publish": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-transformer-notebook"
  },
  "author": "Shaurya Singhal (jugshaurya)",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "jupyter",
    "notebook",
    "notebookjs"
  ],
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-transformer-notebook#readme",
  "dependencies": {
    "notebookjs": "^0.6.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "babel-preset-gatsby-package": "^2.9.0",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "gatsby": "^4.0.0-next"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
